{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Resolution Collection",
    "description": "A Colelction of Resolutions ready to be imported into the Resolution Archive",
    "type": "object",
    "properties": {
        "meta": {
            "type": "object",
            "properties": {
                "version": {
                    "type": "string",
                    "description": "The version of the schema for the collection"
                },
                "created": {
                    "type": "string",
                    "description": "The date the collection was created"
                },
                "council": {
                    "type": "string",
                    "description": "The council that passed the resolutions."
                },
                "count": {
                    "type": "integer",
                    "description": "The number of resolutions in the collection"
                }
            },
            "required": [
                "version"
            ]
        },
        "resolutions": {
            "type": "array",
            "items": {
                "$ref": "resolution.schema.json"
            }
        }
    },
    "required": []
}
